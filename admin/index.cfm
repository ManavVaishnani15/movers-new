<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<html>
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
			
		<link href="css/bootstrap.min.css" type="text/css" rel="stylesheet">
		<link href="css/style.css" type="text/css" rel="stylesheet">
		<title>Login</title>
	</head>

	<body>
		<div class="container-fluid pl-0">
			<div class="row">
				<div class="col">
					<img src="images/New logo.png" class="float-left">
				</div>
			</div>
			
			<form action="main.cfm" method="post" class="w-50 mx-auto text-center mt-5 pt-5">
				<div class="form-group text-left mt-5 pt-5">
					<label for="username" class="text-navy font-weight-bold font-size-125-rem">USERNAME</label>
					<input type="text" class="form-control form-control-lg rounded-0 black-border white-background w-75" id="username" name="username" size="15">
				</div>
				<div class="form-group text-left">
					<label for="password" class="text-navy font-weight-bold font-size-125-rem">PASSWORD</label>
					<input type="password" class="form-control form-control-lg rounded-0 black-border white-background w-75" id="password" name="password" size="15">
				</div>
				<div class="form-group text-center w-75">
					<button type="submit" class="btn black-border rounded-0 text-navy font-weight-bold white-background btn-lg pl-4 pr-4">LOGIN</button>
				</div
			</form>
		</div>
	
		<script type="text/javascript" src="scripts/jquery.min.js"></script>
		<script type="text/javascript" src="scripts/bootstrap.min.js"></script>
	</body>
</html>
